    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        html,body {
            width: 100%;
            height: 100%;
            color: #fff;
            overflow-x: hidden;
            /* overflow: hidden; */
        }
        #nav {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 20px;
            width: 100vw;
            position: fixed;
            height: 10vh;
            background-color: #A2BAE5;
        }
        #nav >h1 {
            font-size: 3.3vw;
            font-family: Six caps;
            font-weight: 100;
        }
        #nav h1 span:nth-child(1) {
            color: red;
            font-family: Arial, sans-serif;
        }
        #nav h1 span:nth-child(2) {
            color: green;
            font-family: Times New Roman, serif;
        }
        #nav h1 span:nth-child(3) {
            color: blue;
            font-family: Verdana, sans-serif;
        }
        #heading{
            display: flex;
            gap: 50px;
            font-size: 1.6vw;
        }
        #heading a{
            text-decoration: none;
            color: white;
        }
        #nav input{
            border: none;
            outline: none;
            border-radius: 20px;
            padding: 12px 20px;
        }
        #nav button{
            border: none;
            outline: none;
            border-radius: 20px;
            padding: 12px 20px;
        }
        #page4 {
            height: 80vh;
            /* scroll-behavior: none; */
            width: 100%;
            position: relative;
            border-radius: 20px;
            animation-name: min;
            position: relative;
            z-index: -99;
/* animation-duration: 8s;
/* display: inline-block; */
/* animation-iteration-count: infinite; */
/* animation-timing-function: linear; */
/* animation-timing-function: linear; */
/* animation-delay: 1s; */
            scroll-behavior: unset;
        }
        
        .swiper-container {
            width: 100%;
            height: 100%;
        }
        .swiper-slide {
            text-align: center;
            font-size: 18px;
            background: #fff;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .swiper-slide img {
            display: block;
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
        @keyframes min {
	0%{
		transform: translateX(0);
		/* color: transparent; */
	}
	100%{
		/* color: black; */
		cursor: pointer;
		transform: translateX(calc(-100% - 30px));
	}
	
}
