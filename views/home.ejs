<script>
    function swiperAnimation() {
        var swiper = new Swiper(".swiper-container", {
            slidesPerView: "auto",
            spaceBetween: 30,
            loop: true,
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
            autoplay: {
                delay: 3000, // Time between each slide in milliseconds
                disableOnInteraction: false, // Continue autoplay even when user interacts with swiper
            },
        });
    }

    swiperAnimation();
    document.querySelector("#div11").addEventListener("click", function(){
        document.querySelector("#div1").style.display = "initial";
        document.querySelector("#div11").style.display = "none";
